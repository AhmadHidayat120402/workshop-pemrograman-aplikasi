    private void NIMActionPerformed(java.awt.event.ActionEvent evt) {                                    
        // TODO add your handling code here:
    }                                   

    private void tb_peminjamanMouseClicked(java.awt.event.MouseEvent evt) {                                           
        // TODO add your handling code here:
         int baris = tb_peminjaman.rowAtPoint(evt.getPoint());
        
        String nim = tb_peminjaman.getValueAt(baris, 1).toString();
        NIM.setText(nim);
        
        String nama = tb_peminjaman.getValueAt(baris, 2).toString();
        Nama.setText(nama);
        
        String jurusan = tb_peminjaman.getValueAt(baris, 3).toString();
        Jurusan.setText(jurusan);
        
        String kode_buku = tb_peminjaman.getValueAt(baris, 4).toString();
        kodebuku.setText(kode_buku);
        
        String judul_buku = tb_peminjaman.getValueAt(baris, 5).toString();
        judulbuku.setText(judul_buku);
        
        String tanggal_terbit = tb_peminjaman.getValueAt(baris, 6).toString();
        tanggalterbit.setText(tanggal_terbit);
        
        String tanggall_pinjam = tb_peminjaman.getValueAt(baris, 7).toString();
        tanggalpinjam.setText(tanggall_pinjam);
    }                                          

    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        try {
            String sql = "INSERT INTO peminjaman VALUES ('" + NIM.getText()+"','"+ Nama.getText()+"','"+ Jurusan.getText()+"','"+ kodebuku.getText()+"','" + judulbuku.getText()+"','"+ tanggalterbit.getText()+"','"+tanggalpinjam.getText()+"')";
            java.sql.Connection conn = new konfig().getConnection();
            java.sql.PreparedStatement pstm = conn.prepareStatement(sql);
            pstm.execute();
            
            JOptionPane.showMessageDialog(null, "berhasil simpan data");
            tampilkan_data();
            kosongkan_form();
            
        } catch (SQLException e) {
            JOptionPane.showMessageDialog(this, e.getMessage());
        }
    
    }                                        

    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
         kosongkan_form();
        NIM.requestFocus();
    }                                        

    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
         try {
            String sql = "DELETE FROM peminjaman WHERE NIM = '"+ NIM.getText()+"'";
            java.sql.Connection conn = new konfig().getConnection();
            java.sql.PreparedStatement pstm = conn.prepareStatement(sql);
            pstm.execute();
            
            JOptionPane.showMessageDialog(null, "Yakin Record Data di hapus ?");
            
        } catch (SQLException e) {
            JOptionPane.showMessageDialog(this, e.getMessage());
        }
        tampilkan_data();
        kosongkan_form();
    }                                        

