  private void kosongkan_form(){
      nim.setText(null);
      nama.setText(null);
      alamat.setText(null);
      notelepon.setText(null);
      tgl_lahir.setDate(null);
      jeniskelamin.setSelectedItem(this);
  }
    private void tampilkan_data (){
        DefaultTableModel model = new DefaultTableModel();
        model.addColumn("no");
        model.addColumn("nim");
        model.addColumn("nama");
        model.addColumn("alamat");
        model.addColumn("no.telepon");
        model.addColumn("Tanggal Lahir");
        model.addColumn("jenis kelamin");  
        
        try {
            int no = 1;
            String sql = "SELECT * FROM mahasiswa";
            java.sql.Connection conn = new konfig().getConnection();
            java.sql.Statement stm = conn.createStatement();
            java.sql.ResultSet rs = stm.executeQuery(sql);
            
            while (rs.next()){
                model.addRow(new Object[] {no++,rs.getString(1),rs.getString(2),rs.getString(3),rs.getString(4),rs.getString(5),rs.getString(6),} );
                
            }
            tb_mahaiswa.setModel(model);
            
        } catch (SQLException e) {
            JOptionPane.showMessageDialog(this, e.getMessage());
        }
        
    }
    
    
  

    /**
     * Creates new form mahasiswa
     */
    public Form_entry_mahasiswa() {
        initComponents();
        tampilkan_data();
        kosongkan_form();
        setLocationRelativeTo(this);
    }
    
   

