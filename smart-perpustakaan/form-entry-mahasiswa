    private void btnTambahActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        kosongkan_form();
        nim.requestFocus();
    }                                         

    private void btnKeluarActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        //System.exit(0);
        {
            new Form_entry_buku().setVisible(true);
            dispose();
    }
    }                                         

    private void tb_mahaiswaMouseClicked(java.awt.event.MouseEvent evt) {                                         
        // TODO add your handling code here:
      
        int baris = tb_mahaiswa.rowAtPoint(evt.getPoint());
        
        String NIM = tb_mahaiswa.getValueAt(baris, 1).toString();
        nim.setText(NIM);
        
        String Nama = tb_mahaiswa.getValueAt(baris, 2).toString();
        nama.setText(Nama);
        
        String Alamat = tb_mahaiswa.getValueAt(baris, 3).toString();
        alamat.setText(Alamat);
        
        String notelp = tb_mahaiswa.getValueAt(baris, 4).toString();
        notelepon.setText(notelp);
        
       
      String TanggalLahir = tb_mahaiswa.getValueAt(baris, 5).toString();
      Date date = null;
      tgl_lahir.setDate(date);
        
        String Jeniskelamin = tb_mahaiswa.getValueAt(baris, 6).toString();
        jeniskelamin.setSelectedItem(Jeniskelamin);
        
    }                                        

    private void btnSimpanActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        try {
            String sql = "INSERT INTO mahasiswa VALUES ('" + nim.getText()+"','"+ nama.getText()+"','"+alamat.getText()+"','"+notelepon.getText()+"','"+tgl_lahir.getDate()+"','"+ jeniskelamin.getSelectedItem()+"')";
            java.sql.Connection conn = new konfig().getConnection();
            java.sql.PreparedStatement pstm = conn.prepareStatement(sql);
            pstm.execute();
            
            JOptionPane.showMessageDialog(null, "berhasil simpan data");
            tampilkan_data();
            kosongkan_form();
            
        } catch (SQLException e) {
            JOptionPane.showMessageDialog(this, e.getMessage());
        }
    }                                         

    private void btnEditActionPerformed(java.awt.event.ActionEvent evt) {                                        
        // TODO add your handling code here:
        try {
            String sql = "UPDATE mahasiswa SET nim = '"+nim.getText()+"', nama = '"+ nama.getText()+"', alamat = '"+alamat.getText()+"', no_telp = '"+notelepon.getText()+"', tgl_lahir = '"+tgl_lahir.getDate()+"', jenis_kelamin = '"+ jeniskelamin.getSelectedItem()+"' WHERE nim = '" + nim.getText()+"'";
            java.sql.Connection conn = new konfig().getConnection();
            java.sql.PreparedStatement pstm = conn.prepareStatement(sql);
            pstm.execute();
            
            JOptionPane.showMessageDialog(null, "berhasil edit data");
            
        } catch (SQLException e) {
            JOptionPane.showMessageDialog(this, e.getMessage());
        }
        tampilkan_data();
        kosongkan_form();
        
    }                                       

    private void btnBatalActionPerformed(java.awt.event.ActionEvent evt) {                                         

        // TODO add your handling code here:
        kosongkan_form();
        nim.requestFocus();
    }                                        

    private void btnHapusActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        try {
            String sql = "DELETE FROM mahasiswa WHERE nim = '"+ nim.getText()+"'";
            java.sql.Connection conn = new konfig().getConnection();
            java.sql.PreparedStatement pstm = conn.prepareStatement(sql);
            pstm.execute();
            
            JOptionPane.showMessageDialog(null, "Yakin Record Data di hapus ?");
            
        } catch (SQLException e) {
            JOptionPane.showMessageDialog(this, e.getMessage());
        }
        tampilkan_data();
        kosongkan_form();
    }                                        

    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        System.exit(0);
    }                                        

    private void tgl_lahirPropertyChange(java.beans.PropertyChangeEvent evt) {                                         
        // TODO add your handling code here:

    }                                        
