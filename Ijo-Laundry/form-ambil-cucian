    }                                               

    private void btn_hapusActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        try {
            String sql = "DELETE FROM tb_ambilcucian WHERE ID_ambilcucian = '"+ tf_idambilcucian.getText()+"'";
            java.sql.Connection conn = new Konfigurasi().getConnection();
            java.sql.PreparedStatement pstm = conn.prepareStatement(sql);
            pstm.execute();

           JOptionPane.showMessageDialog(null, "berhasil hapus data");
        } catch (SQLException e) {
            JOptionPane.showMessageDialog(this, e.getMessage());
        }
        tampilkan_data_ambilcucian();
        id_ambilcucian_otomatis();
        kosongkan_form();
    }                                         

    private void btn_editActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        try {
            String sql = "UPDATE tb_ambilcucian SET ID_ambilcucian = '"+ tf_idambilcucian.getText()+"',ID_transaksi ='"+ tf_transaksi.getText()+"',ID_Pelanggan = '"+ tf_IDpelanggan.getText()+"',nama_pelanggan ='"+ tf_namaPelanggan.getText()+"',no_hp ='"+ tf_Nohp.getText()+"',jenis_kelamin ='"+ (String)JC_JenisKelamin.getSelectedItem()+"',alamat ='"+ tf_alamat.getText()+"',Tanggal_Masuk='"+ JD_tanggalMasuk.getDate()+"',Jenis_Laundry='"+(String)JC_jenisLaundry.getSelectedItem()+"',Harga='"+tf_harga.getText()+"',Berat_Pakaian='"+tf_beratPakaian.getText()+"',Total_Pembayaran='"+tf_totalpembayaran.getText()+"',status='"+(String)JC_status.getSelectedItem()+"' WHERE ID_transaksi = '" + tf_transaksi.getText()+"'";
            java.sql.Connection conn = new Konfigurasi().getConnection();
            java.sql.PreparedStatement pstm = conn.prepareStatement(sql);
            pstm.execute();

            JOptionPane.showMessageDialog(null, "berhasil edit data");
        } catch (SQLException e) {
            JOptionPane.showMessageDialog(this, e.getMessage());
        }
        tampilkan_data_ambilcucian();
        id_ambilcucian_otomatis();
        kosongkan_form();
    }                                        

    private void btn_tambahActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // TODO add your handling code here:
//        kosongkan_form();
        id_ambilcucian_otomatis();
        tf_bayar.requestFocus();
    }                                          

    private void btn_simpanActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // TODO add your handling code here:
         String tampilan = "yyyy-MM-dd";
        SimpleDateFormat fm = new SimpleDateFormat(tampilan);
        String tanggal = String.valueOf(fm.format(JD_tanggalMasuk.getDate()));
        try {
            String sql = "INSERT INTO tb_ambilcucian VALUES ('" +  tf_idambilcucian.getText()+"','"+  tf_transaksi.getText()+"','"+tf_IDpelanggan.getText()+"','"+tf_namaPelanggan.getText()+"','"+tf_Nohp.getText()+"','"+(String)JC_JenisKelamin.getSelectedItem()+"','"+tf_alamat.getText()+"','"+tanggal+"','"+(String)JC_jenisLaundry.getSelectedItem()+"','"+tf_harga.getText()+"','"+tf_beratPakaian.getText()+"','"+tf_totalpembayaran.getText()+"','"+tf_bayar.getText()+"','"+tf_kembalian.getText()+"','"+tanggal+"','"+(String)JC_status.getSelectedItem()+"')";
            java.sql.Connection conn = new Konfigurasi().getConnection();
            java.sql.PreparedStatement pstm = conn.prepareStatement(sql);
            pstm.execute();

            JOptionPane.showMessageDialog(null, "berhasil simpan data");
        } catch (SQLException e) {
            JOptionPane.showMessageDialog(this, e.getMessage());
        }
        tampilkan_data_ambilcucian();
        id_ambilcucian_otomatis();
        kosongkan_form();
    }                                          

