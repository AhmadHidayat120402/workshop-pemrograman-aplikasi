    private void JC_jenisLaundryActionPerformed(java.awt.event.ActionEvent evt) {                                                
        // TODO add your handling code here:
 if(JC_jenisLaundry.getSelectedItem().toString().equalsIgnoreCase("cuci kering reguler 2 hari")){
            tf_harga.setText("4000");
        } else if (JC_jenisLaundry.getSelectedItem().toString().equalsIgnoreCase("cuci kering express 1 hari")){
            tf_harga.setText("5000");
        } else if (JC_jenisLaundry.getSelectedItem().toString().equalsIgnoreCase("cuci basah reguler 2 hari")){
            tf_harga.setText("3000");
        } else if (JC_jenisLaundry.getSelectedItem().toString().equalsIgnoreCase("cuci basah express 1 hari")){
            tf_harga.setText("4000");
        } else if(JC_jenisLaundry.getSelectedItem().toString().equalsIgnoreCase("cuci kering dan setrika reguler 2 hari")){
            tf_harga.setText("6000");
        } else if (JC_jenisLaundry.getSelectedItem().toString().equalsIgnoreCase("cuci kering dan setrika express 1 hari")){
            tf_harga.setText("8000");
        } 
    }                                               

    private void tf_bayarKeyReleased(java.awt.event.KeyEvent evt) {                                     
        // TODO add your handling code here:
        int bayar = Integer.parseInt(tf_bayar.getText());
        int totalbayar = Integer.parseInt(tf_totalpembayaran.getText());
        int kembalian = bayar - totalbayar;
        String kembali = Integer.toString(kembalian);
        tf_kembalian.setText(kembali);
    }                                    

    private void tf_cariiKeyReleased(java.awt.event.KeyEvent evt) {                                     
        // TODO add your handling code here:
        tampilkan_data_ambilcucian();
    }                                    

    private void tabel_ambilcucianMouseClicked(java.awt.event.MouseEvent evt) {                                               
        // TODO add your handling code here:
         int baris = tabel_ambilcucian.rowAtPoint(evt.getPoint());
         
         String ID_ambilcucian = tabel_ambilcucian.getValueAt(baris, 1).toString();
        tf_idambilcucian.setText(ID_ambilcucian);
        
        String ID_transaksi = tabel_ambilcucian.getValueAt(baris, 2).toString();
        tf_transaksi.setText(ID_transaksi);
        
        String ID_Pelanggan = tabel_ambilcucian.getValueAt(baris, 3).toString();
        tf_IDpelanggan.setText(ID_Pelanggan);
        
        String nama_pelanggan = tabel_ambilcucian.getValueAt(baris, 4).toString();
        tf_namaPelanggan.setText(nama_pelanggan);
        
        String no_hp = tabel_ambilcucian.getValueAt(baris, 5).toString();
        tf_Nohp.setText(no_hp);
        
        String jenis_kelamin = tabel_ambilcucian.getValueAt(baris, 6).toString();
        JC_JenisKelamin.setSelectedItem(jenis_kelamin);
        
        String alamat = tabel_ambilcucian.getValueAt(baris, 7).toString();
        tf_alamat.setText(alamat);
        
        JD_tanggalMasuk.setDate(getTanggalFromTable(tabel_ambilcucian, 8));
       
       String Jenis_Laundry = tabel_ambilcucian.getValueAt(baris, 9).toString();
       JC_jenisLaundry.setSelectedItem(Jenis_Laundry);
       
       String Harga = tabel_ambilcucian.getValueAt(baris, 10).toString();
       tf_harga.setText(Harga);
       
       String Berat_Pakaian = tabel_ambilcucian.getValueAt(baris,11).toString();
       tf_beratPakaian.setText(Berat_Pakaian);
       
       String Total_Pembayaran = tabel_ambilcucian.getValueAt(baris, 12).toString();
       tf_totalpembayaran.setText(Total_Pembayaran);
       
       String bayar = tabel_ambilcucian.getValueAt(baris, 13).toString();
       tf_bayar.setText(bayar);
       
       String kembalian = tabel_ambilcucian.getValueAt(baris, 14).toString();
       tf_kembalian.setText(kembalian);
       
       JD_tglPengembalian.setDate(getTanggalFromTable(tabel_ambilcucian, 15));
       
       String status = tabel_ambilcucian.getValueAt(baris, 16).toString();
       JC_status.setSelectedItem(status);
        
    }                                              

