    private void btn_printActionPerformed(java.awt.event.ActionEvent evt) {                                          
//java.io.File namaFile = new java.io.File("./Laporan_pengambilan_Cucian.jasper");
        JasperReport reports;
            String path = ".\\src\\report\\nota.jasper";
        try {
            HashMap hash = new HashMap();
            hash.put("id_transaksi",tf_transaksi.getText());
            reports = (JasperReport) JRLoader.loadObjectFromFile(path);
            JasperPrint jprint = JasperFillManager.fillReport(path,hash,main.Konfigurasi.getConnection());
            JasperViewer jviewer = new JasperViewer(jprint,false);
            jviewer.setDefaultCloseOperation(DISPOSE_ON_CLOSE);
            jviewer.setVisible(true);
            
        } catch (Exception e) {
            JOptionPane.showMessageDialog(null, e.getMessage());
        }
    }                                         

    private void btn_batalActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        kosongkan_form();
        id_ambilcucian_otomatis();
        tf_transaksi.requestFocus();
    }                                         

